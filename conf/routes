GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /                           controllers.HomeController.index

POST    /create                     controllers.HomeController.create

GET     /list                       controllers.HomeController.list

GET     /read/:id                   controllers.HomeController.read(id: reactivemongo.bson.BSONObjectID)

GET     /delete/:id                 controllers.HomeController.delete(id: reactivemongo.bson.BSONObjectID)

PUT     /update/:id                 controllers.HomeController.update(id: reactivemongo.bson.BSONObjectID)

GET     /form                       controllers.HomeController.form